<xenede:menus
 xmlns:xenede="Medusa-Xos-Development-Environment"
 xmlns:mxde="Medusa-Xos-Development-Environment"
 xmlns:xde="Medusa-Xos-Development-Environment"
 xmlns="Medusa-Xos-Development-Environment">

    <size width="170"/>
    <color bgcolor="#dddddd"/>
    <!--========================================================================-->
    <!--                                                                        -->
    <!-- menu-items                                                             -->
    <!--                                                                        -->
    <!--========================================================================-->
    <menu-items>
        <!--========================================================================-->
        <!--                                                                        -->
        <!-- fonts                                                                  -->
        <!--                                                                        -->
        <!--========================================================================-->
        <fonts>
            <xde:font hover="off" face="arial" size="2" color="black"/>
            <xde:font hover="on" face="arial" size="2" color="red"/>
        </fonts>
        <!--========================================================================-->
        <!--                                                                        -->
        <!-- images                                                                 -->
        <!--                                                                        -->
        <!--========================================================================-->
        <images>
            <xde:image id="left" width="2" height="25" location="../images/xenede-menu-item.gif"/>
        </images>
    </menu-items>
    <!--========================================================================-->
    <!--                                                                        -->
    <!-- menu-sections                                                          -->
    <!--                                                                        -->
    <!--========================================================================-->
    <menu-sections
     raise_selected_to_top="yes">
        <xde:size spacing="2"/>
        <xde:color bgcolor="#6688AA"/>
        <!--========================================================================-->
        <!--                                                                        -->
        <!-- fonts                                                                  -->
        <!--                                                                        -->
        <!--========================================================================-->
        <fonts>
            <xde:font hover="off" face="arial" size="2" color="#ffffdd"/>
            <xde:font hover="on" face="arial" size="2" color="#ffffff"/>
        </fonts>
        <!--========================================================================-->
        <!--                                                                        -->
        <!-- images                                                                 -->
        <!--                                                                        -->
        <!--========================================================================-->
        <images>
            <xde:image id="left" state="top" width="13" height="25" location="../images/xenede-menu-top-left.gif"/>
            <xde:image id="left" state="bottom" width="13" height="25" location="../images/xenede-menu-left.gif"/>
        </images>
    </menu-sections>

    <!--========================================================================-->
    <!--                                                                        -->
    <!-- menu                                                                   -->
    <!--                                                                        -->
    <!--========================================================================-->
    &menu-create;

    <!--========================================================================-->
    <!--                                                                        -->
    <!-- menu                                                                   -->
    <!--                                                                        -->
    <!--========================================================================-->
    &menu-project;

    <!--========================================================================-->
    <!--                                                                        -->
    <!-- menu                                                                   -->
    <!--                                                                        -->
    <!--========================================================================-->
    &menu-tools;

    <!--========================================================================-->
    <!--                                                                        -->
    <!-- menu                                                                   -->
    <!--                                                                        -->
    <!--========================================================================-->
    &menu-source;

    <!--========================================================================-->
    <!--                                                                        -->
    <!-- menu                                                                   -->
    <!--                                                                        -->
    <!--========================================================================-->
    &menu-documents;

    <!--========================================================================-->
    <!--                                                                        -->
    <!-- menu                                                                   -->
    <!--                                                                        -->
    <!--========================================================================-->
    &menu-help;

</xenede:menus>
