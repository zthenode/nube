    </tab>
    <!--========================================================================-->
    <!-- tab                                                                    -->
    <!--========================================================================-->
    <tab id="source" menu="source">
        <text>Source</text>
    </tab>
    <!--========================================================================-->
    <!-- tab                                                                    -->
    <!--========================================================================-->
    <tab id="documents" menu="documents">
        <text>Documents</text>
    </tab>
    <!--========================================================================-->
    <!-- tab                                                                    -->
    <!--========================================================================-->
    <tab id="help" menu="help">
        <text>Help</text>
    </tab>

