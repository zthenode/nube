<medusaxde:menu-section
 xmlns:medusaxde="Medusa-Xos-Development-Environment"
 xmlns:mxde="Medusa-Xos-Development-Environment"
 xmlns:xde="Medusa-Xos-Development-Environment"
 xmlns="Medusa-Xos-Development-Environment"
 id="create_t"
 using="t" or_not_using="yes">
    <using yes="xsl"/>

    <xde:text>Create T</xde:text>
    <!--========================================================================-->
    <!-- menu-item                                                              -->
    <!--========================================================================-->
    <menu-item id="t_template"
         is_location_query_and="yes" 
         is_location_query_and_for="yes" 
         is_location_query_and_using="yes" 
         location_path="/medusaxde/xsl/t/"
         location_query="document_file=../medusaxde.xml&amp;form=1" 
         location="medusaxde-create-t-file.xsl">
        <xde:text>File</xde:text>
    </menu-item>
    <!--==========================================================-->
    <xde:menu-item is_separator="yes"/>
    <!--========================================================================-->
    <!-- menu-item                                                              -->
    <!--========================================================================-->
    <menu-item id="t_if"
         is_location_query_and="yes" 
         is_location_query_and_for="yes" 
         is_location_query_and_using="yes" 
         location_path="/medusaxde/xsl/t/"
         location_query="document_file=../medusaxde.xml&amp;form=1" 
         location="medusaxde-create-t-if.xsl">
