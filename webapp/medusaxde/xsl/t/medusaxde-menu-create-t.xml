         location_query="document_file=../medusaxde.xml&amp;form=1" 
         location="medusaxde-create-t-case.xsl">
        <xde:text>%case()%</xde:text>
    </menu-item>
    <!--========================================================================-->
    <!-- menu-item                                                              -->
    <!--========================================================================-->
