         location_query="document_file=../medusaxde.xml&amp;form=1" 
         location="medusaxde-create-t-parse.xsl">
        <xde:text>%parse()%</xde:text>
    </menu-item>
    <!--========================================================================-->
    <!-- menu-item                                                              -->
    <!--========================================================================-->
    <menu-item id="t_parameters"
         is_location_query_and="yes" 
         is_location_query_and_for="yes" 
         is_location_query_and_using="yes" 
         location_path="/medusaxde/xsl/t/"
         location_query="document_file=../medusaxde.xml&amp;form=1" 
         location="medusaxde-create-t-parameters.xsl">
        <xde:text>%parameters()%</xde:text>
    </menu-item>
    <!--==========================================================-->
    <xde:menu-item is_separator="yes"/>
</medusaxde:menu-section>
